{
    "name": "dogebot",
    "version": "0.1.0",
    "description": "DogeBot",
    "scripts": {
        "lint": "eslint .",
        "format": "eslint . --fix",
        "typecheck": "tsc",
        "build": "tsc --noEmit false",
        "dev": "tsc-watch --onSuccess \"tsx ./api/bot.ts\"",
        "start": "tsx ./api/bot.ts",
        "start:force": "tsx ./api/bot.ts",
        "prepare": "npx husky install"
    },
    "devDependencies": {
        "@types/node": "^22.10.5",
        "tsc-watch": "^6.2.1",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "dotenv": "^16.4.7",
        "grammy": "^1.34.0",
        "tsx": "^4.19.2"
    }
}
